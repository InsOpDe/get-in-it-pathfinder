{
  "name": "get-in-it-entry",
  "version": "1.0.0",
  "main": "app.js",
  "license": "MIT",
  "private": true,
  "dependencies": {},
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/express": "*",
    "@types/mocha": "^5.2.4",
    "@types/node": "^12.0.2",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "mocha": "^5.0.1",
    "nyc": "^13.0.1",
    "reflect-metadata": "^0.1.12",
    "source-map-support": "^0.5.3",
    "ts-node": "^7.0.0",
    "typescript": "*"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "build/*",
      "coverage/*",
      "src/tools/*",
      "**/*.d.ts",
      "**/*.db.ts",
      "**/*.spec.ts"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  },
  "scripts": {
    "test:textcoverage": "nyc --reporter=text-summary mocha -r ts-node/register --slow 50 'src/*/**/*.spec.ts' --timeout 10000",
    "start": "node dist/app.js",
    "buildAndStart": "tsc && node dist/app.js"
  }
}
